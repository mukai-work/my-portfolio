{
  "name": "my-portfolio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node ./scripts/run-nuxt.mjs build",
    "dev": "node ./scripts/run-nuxt.mjs dev --host 0.0.0.0 --port 3000",
    "generate": "node ./scripts/run-nuxt.mjs generate",
    "preview": "node ./scripts/run-nuxt.mjs preview",
    "nuxt:prepare": "node ./scripts/run-nuxt.mjs prepare",
    "postinstall": "node ./scripts/run-nuxt.mjs prepare",
    "lint": "eslint . --ext .ts,.js,.vue",
    "lint:fix": "eslint . --ext .ts,.js,.vue --fix",
    "format": "prettier --write \"**/*.{ts,js,vue,css,md,json}\"",
    "typecheck": "vue-tsc --noEmit",
    "test": "pnpm test:unit",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "echo 'Playwright tests are no longer available' && exit 0",
    "e2e": "pnpm test:e2e"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@vueuse/core": "^11.2.0",
    "nuxt": "^3.17.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitest/coverage-c8": "^0.33.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.32.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "vitest": "^2.1.6",
    "vue-tsc": "^2.1.10"
  }
}
