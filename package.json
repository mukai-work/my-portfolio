{
  "name": "my-portfolio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host 0.0.0.0 --port 3000",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && prisma generate",
    "lint": "eslint . --ext .ts,.js,.vue",
    "lint:fix": "eslint . --ext .ts,.js,.vue --fix",
    "format": "prettier --write \"**/*.{ts,js,vue,css,md,json}\"",
    "typecheck": "vue-tsc --noEmit",
    "test": "pnpm test:unit",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "pnpm prisma migrate deploy --schema prisma/schema.prisma && playwright test",
    "e2e": "pnpm test:e2e",
    "prepare:e2e": "playwright install --with-deps",
    "seed": "prisma db seed",
    "migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@octokit/rest": "^21.0.2",
    "@prisma/client": "^5.22.0",
    "@vueuse/core": "^11.2.0",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.17.2",
    "openai": "^4.79.1",
    "pdf-lib": "^1.17.1",
    "pino": "^9.5.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "zod": "^3.23.8",
    "vue-i18n": "^9.14.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitest/coverage-v8": "^2.1.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.32.0",
    "playwright": "^1.49.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.6",
    "vue-tsc": "^2.1.10",
    "pino-pretty": "^11.3.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}